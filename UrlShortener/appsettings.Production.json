{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/urlshortener.db"
  },
  "BaseUrl": "http://localhost:8080",
  "JwtSettings": {
    "Secret": "your-super-secret-key-that-is-at-least-32-characters-long-for-security",
    "Issuer": "UrlShortenerApp",
    "Audience": "UrlShortenerUsers",
    "ExpiryInDays": 7
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
